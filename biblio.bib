  shorttitle = {The {{Global Warming Potential}} of {{Building Materials}}},
  author = {Bhochhibhoya, Silu and Zanetti, Michela and Pierobon, Francesca and Gatto, Paola and Maskey, Ramesh Kumar and Cavalli, Raffaele},
